confirmation.ZIndex = 3
confirmation.Font = Enum.Font.SourceSansBold
confirmation.Text = "Confirm?"
confirmation.TextColor3 = Color3.fromRGB(0, 170, 0)
confirmation.TextSize = 32.000
confirmation.MouseButton1Down:Connect(function()
	local Sound = Instance.new("Sound")
	Sound.SoundId = "rbxassetid://4612384434"
	Sound.Looped = false
	Sound:Play()

	confirmation.Visible = false
	StartBlocks.Visible = true
	confirmation.Active = true
	Title.Active = true

	game.StarterGui:SetCore("SendNotification", {
		Title = "Execute It Multiple Times.";
		Decal = "rbxassetid://4871684516";
		Text = "For Faster And More Blocks";
		Duration = 10;
	})

	while confirmation.Active do
		for i = 1,100 do
			wait()
			local plr = game:GetService("Players").LocalPlayer

			local args = {[1] = "Turkey"} workspace.Events.Morph.Player:FireServer(unpack(args))

			wait()

			for i,l in pairs(plr.Character.Torso:GetChildren()) do
				if l:IsA("Part") and l.Name == "Part"  and l:FindFirstChildWhichIsA("SpecialMesh") then
					l:FindFirstChildWhichIsA("SpecialMesh"):Destroy()
				end end
			for i,v in pairs(plr.Character:FindFirstChild("Torso"):GetChildren()) do
				if v.Name == "Weld" and v:IsA("Weld")  then
					v:Destroy()
					for i,g in pairs(plr.Character:GetChildren()) do
						if g:IsA("StringValue")  then
							g:Destroy()
							wait:Destroy()
						end  end end end end
		wait(5)
	end
end)

