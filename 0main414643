local function PGXBUFF_fake_script() -- timeel.LocalScript 
	local script = Instance.new('LocalScript', timeel)

	local Maxseconds = 999999999999999999999999999999999999999999999999999999999999999999999

	local TextLabel = timeel

	for i = 1, Maxseconds,1 do
		wait(1)
		local minutes = math.floor(i/60)
		local hours = math.floor(minutes/60)
		local seconds = i - (minutes * 60)
		minutes -= hours * 60
		TextLabel.Text = hours.."h: "..minutes.."m: "..seconds.."s"
	end
end
coroutine.wrap(PGXBUFF_fake_script)()

