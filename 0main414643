Stop.MouseButton1Down:connect(function()
	local Sound = Instance.new("Sound")
	Sound.SoundId = "rbxassetid://4612384434"
	Sound.Looped = false
	Sound:Play()
	settings():GetService("NetworkSettings").IncomingReplicationLag = 0
	ServicePlayer.Character.Humanoid.RigType = 'R15'
	strollertrue.Visible = false
	Strollerfalse.Visible = true
	confirmation.Active = false
	Title.Active = false
	if frtrue.Visible == true then
		game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.Anchored = true
	else
		game:GetService('Players').LocalPlayer.Character.HumanoidRootPart.Anchored = false
	end
	game.StarterGui:SetCore("SendNotification", {
		Icon = "rbxthumb://type=Asset&id=5107182114&w=150&h=150";
		Title = "Please Wait.";
		Decal = "rbxassetid://4871684516";
		Text = "Attempting To Stop.";
		Duration = 10;
	})

	Stop.Visible = false
	Execute.Visible = true
	StartBlocks.Visible = true
	confirmation.Visible = false

	game:GetService('Players').LocalPlayer.Character.Humanoid.Health = 0
	game:GetService('Players').LocalPlayer.Character.Humanoid.Sit = true
	local noclip = true char = game.Players.LocalPlayer.Character while true do if noclip == true then for _,v in pairs(char:children()) do pcall(function() if v.className == "Part" then v.CanCollide = false elseif v.ClassName == "Model" then v.Head.CanCollide = false end end) end end game:service("RunService").Stepped:wait() end
	settings():GetService("NetworkSettings").IncomingReplicationLag = 0
end)

