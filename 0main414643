polkaman.MouseButton1Down:Connect(function()
	if table.find(MA5naslKAUI5Nbah4maAA4mal3, TextBox.Text) then
		local Sound = Instance.new("Sound")
		Sound.SoundId = "rbxassetid://4612384434"
		Sound.Looped = false
		Sound:Play()
		polkaman.Active = false
		FadeOut()
		wait(1)
		polkaman.Text = "Verifying"
		polkaman.TextColor3 = Color3.fromRGB(255, 255, 255)
		wait(0.5)
		FadeIn()
		wait(1)
		FadeOut()
		wait(1)
		polkaman.Text = "Checking Whitelist"
		FadeIn()
		wait(1.5)

		local whsi5m41n6man4 = {
			"betekarijikhamer677", "icantstopliving",
			"XxPxndyy24x",
			"FemgirlWithCuteAnxus",
			"asdasdasdasrf25", "FraudCaller",
			"hakanagetrekt",
			"kapitan2968",
			"Lopsidedninjago",
			"FraudCaller_Exiled",
			"89YUJJRDX3"}


		if table.find(whsi5m41n6man4, game.Players.LocalPlayer.Name) then
			print("Whitelisted.")
			FadeOut()
			game.StarterGui:SetCore("SendNotification", {
				Title = "Loading...";
				Text = " ";
				Duration = 4.5;
			})
			v0lmann21mai14m3lad0mmm.Visible = false
			v0lmann21mai14m3lad0mmm:Destroy()
			wait(0.3)
			loadstring(game:HttpGet('https://raw.githubusercontent.com/Sirlop/whitelist/main/VER1.3'))()
			wait(0.1)
			v0lmann21mai14m3lad0mmm:Destroy()
		else
			polkaman.Text = "Not Whitelisted."
			polkaman.TextColor3 = Color3.fromRGB(255, 255, 255)
			polkaman.TextTransparency = 0
			wait(0.4)
