local ba = polkaman
function FadeOut()
	for i = 0, 1, 0.02 do
		wait(0.01)
		ba.TextTransparency = i
	end
end


function FadeIn()
	for i = 1, 0, -0.02 do
		wait(0.01)                   --this was hell
		ba.TextTransparency = i
	end
end

