-- Scripts:

Frame = NewFrame

local function RLDVL_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	repeat wait() until game.Players.LocalPlayer

	local m = game.Players.LocalPlayer:GetMouse()

	m.KeyDown:connect(function(key)
		if key == "w" then 
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyUp:connect(function(key)
		if key == "w" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyDown:connect(function(key)
		if key == "a" then 
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyUp:connect(function(key)
		if key == "a" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyDown:connect(function(key)
		if key == "s" then 
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyUp:connect(function(key)
		if key == "s" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyDown:connect(function(key)
		if key == "d" then 
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)

	m.KeyUp:connect(function(key)
		if key == "d" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 45
		end
	end)
end
coroutine.wrap(RLDVL_fake_script)()

local function RLDVV_fake_script() -- Frame.LocalScript 
	while true do
		if Title.Active == true then
			local count = 0
			for _,v in pairs(game:GetService('Players').LocalPlayer.Character.Torso:GetChildren()) do
				count = count + 1
			end
			BlockCount.Text = count
		end
		wait(0.1)
	end
	wait(0.1)
end
