godf.Text = "False"
godf.TextColor3 = Color3.fromRGB(234, 0, 4)
godf.TextSize = 21.000
function GetDescendantsOfClass(Parent, Type)
	local Table = {}

	for _, Object in ipairs(Parent:GetDescendants()) do
		if Object:IsA(Type) then
			table.insert(Table, Object)
		end
	end

	return Table
end

