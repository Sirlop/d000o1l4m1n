StartBlocks.Name = "StartBlocks"
StartBlocks.Parent = HomeScroll
StartBlocks.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
StartBlocks.BackgroundTransparency = 1.000
StartBlocks.Position = UDim2.new(0.165000007, 0, 0.452174604, 0)
StartBlocks.Size = UDim2.new(0, 200, 0, 50)
StartBlocks.ZIndex = 3
StartBlocks.Font = Enum.Font.SourceSansBold
StartBlocks.Text = "Start"
StartBlocks.TextColor3 = Color3.fromRGB(0, 170, 0)
StartBlocks.TextSize = 32.000
StartBlocks.MouseButton1Down:Connect(function()
	local Sound = Instance.new("Sound")
	Sound.SoundId = "rbxassetid://4612384434"
	Sound.Looped = false
	Sound:Play()
	Title.Active = false
	StartBlocks.Visible = false
	confirmation.Visible = true

	for i = 1,100 do
		wait()
		local plr = game:GetService("Players").LocalPlayer

		local args = {[1] = "Turkey"} workspace.Events.Morph.Player:FireServer(unpack(args))

		wait()

		for i,l in pairs(plr.Character.Torso:GetChildren()) do
			if l:IsA("Part") and l.Name == "Part"  and l:FindFirstChildWhichIsA("SpecialMesh") then
				l:FindFirstChildWhichIsA("SpecialMesh"):Destroy()
			end end
		for i,v in pairs(plr.Character:FindFirstChild("Torso"):GetChildren()) do
			if v.Name == "Weld" and v:IsA("Weld")  then
				v:Destroy()
				for i,g in pairs(plr.Character:GetChildren()) do
					if g:IsA("StringValue")  then
						g:Destroy()
						wait:Destroy()
					end  end end end end


end)


